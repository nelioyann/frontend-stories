<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ meta.site_name + " - " + title or " " }}</title>
    <meta name="description" content="{{description or meta.site_description}}">
    <link rel="stylesheet" href="/css/style.css">
    <meta name="theme-color" content="{{meta.theme_color}}">
    <meta name="apple-mobile-web-app-status-bar-style" content="{{meta.theme_color}}">
    <meta name="title" content="{{meta.site_name + " - " + title or " "}}">
    <meta name="generator" content="{{ eleventy.generator }}">
    <!-- Open graph - Facebook 1200 x 628 -->
    <meta property="og:image" content="{{thumbnail or meta.site_thumbnail}}">
    <meta property="og:site_name" content="{{meta.site_name}}">
    <meta property="og:type" content="{{og_type}}">
    <meta property="og:url" content="{{page.url or meta.site_url}}">
    <meta property="og:title" content="{{title or meta.site_name}}">
    <meta property="og:description" content="{{description or meta.site_description}}">
    <!-- Open graph - Twitter -->
    <meta name="twitter:image" content="{{thumbnail or meta.site_thumbnail}}">
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:creator" content="{{meta.twitter_name}}">
    <meta name="twitter:site" content="{{meta.twitter_name}}">
    <meta name="twitter:text:title" content="{{meta.site_name + " - " + title or " " }}">


  <link rel="manifest" href="/manifest.json" />

    <!-- Favicon -->
    {% if emojicon %}
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 100 100%22><text y=%22.9em%22 font-size=%2290%22>{{emojicon}}</text></svg>">
    {% else%}
        <link rel="shortcut icon" href="/images/icons/icon-16.png" type="image/x-icon" sizes="16x16" />
        <link rel="shortcut icon" href="/images/icons/icon-32.png" type="image/x-icon" sizes="32x32" />
        <link rel="icon" type="image/png" sizes="196x196" href="images/icons/icon-196.png">
        <link rel="apple-touch-icon" href="images/icons/icon-180.png">
    {% endif %}
    {# Author etc #}
    <meta name="author" content="{{meta.site_author}}">

</head>