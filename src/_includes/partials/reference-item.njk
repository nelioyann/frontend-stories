<li class="reference-item" data-props="noMeasure">
    <a data-button="link" rel="noopener nofollow noreferrer" target="{{ '_blank' if reference.url }}" href="{{ (reference.url if reference.url else '#') }}">
        <span>
            {{ reference.name }}
        </span>
    </a>
    | {{ reference.cite if reference.cite }}
</li>