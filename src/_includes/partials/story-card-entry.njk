<article>
    <a class="headline-entry" data-heading="3" data-font="sans-serif" itemprop="url" data-button="link" href="/{{ entry.slug }}" data-icon="{{entry.icon}}">
        <span data-font="emoji" aria-hidden="true">
            {{entry.icon}}
        </span>
        <b>
            {{ entry.name }}
        </b>
    </a>
</article>