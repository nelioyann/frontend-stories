<article>
    <a class="headline-entry" data-font="sans-serif" itemprop="url" data-button="link" href="/{{ entry.slug }}" data-icon="{{entry.icon}}">
        <span aria-hidden="true">
            {{entry.icon}}
        </span>
        <b>
            {{ entry.name }}
        </b>
    </a>
    <p>
        {{entry.excerpt}}
    </p>
</article>